<% if @branch %>
<div>
  <% audios = audio_players(@branch) %>
   <% audios.each do |a| %>
     <span><b><%= a[0] %></b></span>
     <span><%= a[1] %></span>
     <span><%= a[2] %></span>
     <br/></br/>
  <% end %>
</div>
<script>
jQuery(document).ready(function(){
  $(".audio").mb_miniPlayer({
    width:240,
    inLine:false,
    id3:false
  });
});
</script>
<% end %>
