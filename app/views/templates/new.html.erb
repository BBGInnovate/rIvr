<%= render :partial=>'upload', 
   :locals=>{:template=>@template, 
   :prompt=>"#{@template.name.titleize} MP3" } %>
<script>
  jQuery(document).ready(function () { 
  // pass forum type
    reportUpload.init("<%= @template.class.name.downcase %>");
    // jquery combobox for Voting Session Name 
    // create or select
    $("[id*='identifier']").combobox();
    $(".audio, .audio2").mb_miniPlayer({
       width:100,
       inLine:false,
       id3:false
  });
  });
</script>
