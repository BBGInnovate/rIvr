
  <%= form_for @option, :url=> headline_templates_path, :html => {:method=>'post',:id=>"frm-upload-logo"} do |f| %>
  <%= f.hidden_field :branch %>
  <% options = [['dropbox','dropbox'],['static_rss','static_rss']]
  id = @option.branch %>
  
  <h2>Select a Headline News Source:</h2>
  <div>
    <%= f.select :feed_source, options_for_select(options, id), :style=>'' %>
  </div>
  <div>
    <span><%= submit_tag 'Save', :class=>"btnAction" %></span>
    <span><%= submit_tag 'Cance', :class=>"btnAction" %></span>
  </div>
  <% end %>

