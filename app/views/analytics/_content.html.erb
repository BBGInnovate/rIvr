<a href="#" id="send-email" class="print-it">Email</a>
<%= link_to_function("Print", "javascript:print()", :class=>"print-it") %>
<h2 class="print-header"><%= @report_name %></h2>
<span class="print-date"><%= @start_date %> - <%= @end_date %> </span>
<table class="search-results-table tablesorter">
  <thead>
    <tr>
    	<% @title.each do |f| %>
      <th data-id="<%= f.lstrip.rstrip %>">
        <%= f %>
      </th>
      <% end %>
    </tr>
  </thead>
  <tbody>
    <% @rows.each do | row | %>
    <tr>
      <% @title.each do |f| %>
      <td>
        <%= row[f] %>
      </td>
      <% end %>
    </tr>
    <% end %>
  </tbody>
</table>
<table class="search-results-table tablesorter">
  <thead>
    <tr>
      <% @country_title.each do |f| %>
      <th>
        <%= f %>
      </th>
      <% end %>
    </tr>
  </thead>
  <tbody>
    <% @country_rows.each do | row | %>
    <tr>
      <% @country_title.each do |f| %>
      <td>
        <%= row[f] %>
      </td>
      <% end %>
    </tr>
    <% end %>
  </tbody>
</table>


